{
    "manifestVersion": 1,
    "id": "devops-github-extension",
    "name": "Github AzureDevOps Rest Api Integration",
    "version": "0.2.1",
    "publisher": "jikuma",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Github AzureDevOps Rest Api Integration. This task enable you to call github rest api using azure devops github endpoint. This enables you to integrate your CI/CD pipeline better with github.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "githubrestapitask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "githubrestapitask"
            }
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license-terms.md"
        }
    },
    "screenshots": [
        {
            "path": "screenshots/screen1.png"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/jikuma/AzureDevOpsGithubRestApi"
    },
    
    "links": {
        "repository": {
            "uri": "https://github.com/jikuma/AzureDevOpsGithubRestApi"
        }
    }
}
